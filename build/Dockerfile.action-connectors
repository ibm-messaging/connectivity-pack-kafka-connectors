FROM us.icr.io/conn-pack-prod-ns/action-connectors@sha256:7d6929a79d9958d0c444fa46980e1cd7e1262f9f9dfe88338a37592e4fef14ed

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001