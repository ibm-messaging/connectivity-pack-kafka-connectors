FROM us.icr.io/conn-pack-prod-ns/action-connectors@sha256:77ee5619e70911c9fd9570ac9da8552e0fa6862334f4a7b26afe4fc7a2ed2889

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001