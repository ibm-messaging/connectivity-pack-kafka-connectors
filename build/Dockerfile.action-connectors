FROM us.icr.io/conn-pack-prod-ns/action-connectors@sha256:f967881eba29ba104ebc0cd8012cfb68fc1952e24c906f03eb25f2875535816a

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001