FROM us.icr.io/conn-pack-prod-ns/event-connectors@sha256:f8ebc4d44f886f8807caeb32a4b7053da8560caab05768851b5ef6f69b632579

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001