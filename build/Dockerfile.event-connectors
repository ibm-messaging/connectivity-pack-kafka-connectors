FROM us.icr.io/conn-pack-prod-ns/event-connectors@sha256:378dc102c28bce462dc12f1909df20cb6d543c4be5b3cf299cba5b03faae9b3d

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001