FROM us.icr.io/conn-pack-prod-ns/event-connectors@sha256:bedc7aa6a4735050a164694dfb16350a97baa809a1a4fef56bbb9951b8a62acb

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001