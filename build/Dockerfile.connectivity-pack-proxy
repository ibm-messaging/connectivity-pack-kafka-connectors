FROM us.icr.io/conn-pack-prod-ns/connectivity-pack-proxy@sha256:624e235d5008796434c53fe82750e51e85a47d40e6156eae9948e8a9f042acb1

USER root

# Add license files
RUN rm -rf licenses/ && \
    mkdir -p /licenses
COPY ./build/licenses /licenses
# package in common license viewer script for pretty license printing
COPY ./build/view_licenses.sh /

USER 1001